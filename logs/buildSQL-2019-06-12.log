CREATE TABLE WJ_DEPUTE_TRAN (
    INCFLG CHAR(1),
    CURWKDY CHAR(8),
    TRADE_NO VARCHAR2(20) NOT NULL,
    TRADE_TYPE VARCHAR2(1),
    PRICE_TYPE VARCHAR2(1),
    TRADE_WEIGHT NUMBER(16,4),
    ACCOUNT_NO VARCHAR2(20),
    TRAN_DATE DATE,
    TRADE_PRICE NUMBER(16,2),
    ISSUE_NO VARCHAR2(20),
    ISSUE_RATE NUMBER(16,4),
    OPTION_RATE NUMBER(16,2),
    FLOAT_RATE NUMBER(16,2),
    LIMIT_CYCLE NUMBER(6),
    LIMIT_UNIT VARCHAR2(1),
    START_BONUS_DATE VARCHAR2(8),
    EXP_DATE VARCHAR2(8),
    POSTPONE_DATE VARCHAR2(8),
    EXP_STATUS VARCHAR2(2),
    BONUS_ALREADY_PAY NUMBER(16,4),
    BONUS_NEED_PAY NUMBER(16,4),
    TRADE_WAY VARCHAR2(1),
    TRADE_STATUS VARCHAR2(2),
    CANCLE_DATE DATE,
    TRADE_DESC VARCHAR2(200),
    TRADE_AMT NUMBER(16,2),
    TRADE_DATE DATE,
    CURRENT_TRAN_NO VARCHAR2(20),
    OPTION_TRAN_NO VARCHAR2(20),
    CUST_CARD_NO VARCHAR2(40),
    QE_STATUS VARCHAR2(1),
    EXP_DAY_SHPRICE NUMBER(16,2),
    EXP_DAY_SELL_NO VARCHAR2(20),
    IS_EXEC VARCHAR2(1),
    BANCS_NO VARCHAR2(20),
    BANK_ID VARCHAR2(10),
    TELLER VARCHAR2(20),
    AUTHOR VARCHAR2(20),
    REMARK VARCHAR2(200),
    BONUS_STATUS VARCHAR2(1),
    CHECK_FLAG VARCHAR2(1),
    WPT_NEXT_DATE VARCHAR2(8),
    CHANGE_STATUS_TIME DATE,
    TOP_HEAD VARCHAR2(20),
    BANCS_CUSTNO VARCHAR2(40),
    ISSUE_TYPE VARCHAR2(2),
    ISSUE_KIND VARCHAR2(2)
);
COMMENT ON COLUMN WJ_DEPUTE_TRAN.INCFLG IS '增量/全量方式';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CURWKDY IS '日期';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_NO IS '申购流水号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_TYPE IS '申购类型(1-人民币购买 2-活期转可转)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.PRICE_TYPE IS '申购执行价类型(1-上海金 2-手工价)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_WEIGHT IS '申购份额';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.ACCOUNT_NO IS '内部客户号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRAN_DATE IS '挂单交易时间';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_PRICE IS '执行价格';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.ISSUE_NO IS '产品编码';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.ISSUE_RATE IS '产品利率';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.OPTION_RATE IS '成交期权费率';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.FLOAT_RATE IS '上浮利率';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.LIMIT_CYCLE IS '产品期限数';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.LIMIT_UNIT IS '期限单位(0-天 1-周 2-月)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.START_BONUS_DATE IS '起息日';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.EXP_DATE IS '到期日';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.POSTPONE_DATE IS '顺延日期';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.EXP_STATUS IS '到期状态(0-未到期 1-到期转人民币 2-到期转积利金)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BONUS_ALREADY_PAY IS '已付利息';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BONUS_NEED_PAY IS '当期应付利息';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_WAY IS '交易渠道(0-柜面,1-网银,2-手机银行,3-BL)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_STATUS IS '交易状态(0-待成交 1-已成交 2-已撤销 3-处理失败 4-未明)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CANCLE_DATE IS '撤单日期';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_DESC IS '成交结果处理说明';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_AMT IS '执行金额, 执行金额*份额';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TRADE_DATE IS '成交执行时间';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CURRENT_TRAN_NO IS '关联活期交易流水';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.OPTION_TRAN_NO IS '关联期权交易流水';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CUST_CARD_NO IS '客户关联资金账户';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.QE_STATUS IS '送下游头寸状态(0-成功 1-失败 2-补送成功)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.EXP_DAY_SHPRICE IS '到期日上海金午盘价';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.EXP_DAY_SELL_NO IS '行权卖出流水号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.IS_EXEC IS '是否行权,(1-是 2-否)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BANCS_NO IS '成交核心交易流水号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BANK_ID IS '交易机构';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TELLER IS '操作经办';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.AUTHOR IS '操作复核';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.REMARK IS '备注';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BONUS_STATUS IS '红利结算状态(0-未结算 1-已结算)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CHECK_FLAG IS '验证当前挂单交易账户余额是否足够标识(0-否，1-是)';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.WPT_NEXT_DATE IS '挂单执行日期';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.CHANGE_STATUS_TIME IS '状态改变记录时间,配合DCDS下传需要';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.TOP_HEAD IS '省行机构号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.BANCS_CUSTNO IS '核心客户号';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.ISSUE_TYPE IS '产品大类';
COMMENT ON COLUMN WJ_DEPUTE_TRAN.ISSUE_KIND IS '产品小类';
ALTER TABLE WJ_DEPUTE_TRAN ADD PRIMARY KEY(TRADE_NO);

